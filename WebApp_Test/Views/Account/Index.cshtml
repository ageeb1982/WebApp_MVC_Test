@model IEnumerable<WebApp_Test.Models.MyUsers>

@{
    ViewBag.Title = "User List";
    if (!string.IsNullOrEmpty(ViewBag.Text)) { ViewBag.Text = ViewBag.Title; }
}


@Html.Partial("_UpX")



<h2>@ViewBag.Title</h2>



<p>
    @Html.ActionLink("New User", "Register")
</p>
<h2>Users</h2>

<table id="T1" class="table table-bordered table-hover">
    <thead>
        <tr>


            <th>
                @Html.DisplayNameFor(mod => mod.First().UserName)
            </th>
            <th>
                @Html.DisplayNameFor(mod => mod.First().Email)
            </th>



            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>


                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                 
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>

                <td>
                 @Html.ActionLink("Edit", "Edit", new { id = item.Id })
                </td>


            </tr>

        }
    </tbody>
</table>

 
@Html.Partial("_DownX")

<script type="text/javascript">
$(document).ready(function () {
    $('#T1').DataTable()
    
  })
    </script>